[ req ]
default_bits       = 2048
distinguished_name = req_distinguished_name
x509_extensions    = v3_req
prompt             = no
string_mask        = utf8only

[ req_distinguished_name ]
CN = bshorts.local

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = bshorts.local
IP.1  = 127.0.0.1
